<?xml version="1.0" encoding="UTF-8"?>
<webServices
    xmlns="http://java.sun.com/xml/ns/jax-rpc/ri/dd"
    version="1.0"
    targetNamespaceBase="http://localhost/wsdl"
    typeNamespaceBase="http://localhost/types"
    urlPatternBase="/articulos">

    <endpoint
        name="ArticulosService"
        displayName="Servicio de articulos"
        description="Un servicio web de articulos de regalo" 
        interface="ogs.ArticuloIF"  
        implementation="ogs.ArticuloImpl"/> 
 
    <endpointMapping
        endpointName="ArticulosService"
        urlPattern="/articulos"/>

</webServices> 
